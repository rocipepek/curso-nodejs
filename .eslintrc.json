{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "arrow-parens": "off",
    "spaced-comment": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": "off"
  }
}
